
<%= form_for @filament do |f| %>
  <%= f.label(:material, 'Material') %>
  <%= f.text_field :material %>

  <%= f.label :color %>
  <%= f.text_field :color %>

  <%= f.label :brand %>
  <%= f.text_field :brand %>

  <%= f.label(:fullWeight, 'Spool size') %>
  <%= f.number_field :fullWeight, :step => :any %>

  <%= f.submit "Submit" %>
<% end %>
